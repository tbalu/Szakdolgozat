        </columns>
      </TableView>
      <Button fx:id="gepjarmuveiButton" layoutX="435.0" layoutY="443.0" mnemonicParsing="false" onAction="#gepjarmuveiButtonPushed" text="Gépjarművei" />
      <TextField fx:id="nevTextField" layoutX="223.0" layoutY="45.0" />
      <Label fx:id="nevLabel" layoutX="100.0" layoutY="50.0" text="Név" />
      <Button fx:id="keresButton" layoutX="435.0" layoutY="45.0" mnemonicParsing="false" onAction="#keresButtonPushed" text="Keres" />
